{
  "expo": {
    "name": "swiftab-app",
    "slug": "swiftab-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "splash": {
      "image": "./assets/images/adaptive-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#008080"
    },
    "scheme": "swiftabapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "NSCameraUsageDescription": "$(Swiftab) needs access to your Camera.",
        "NSMicrophoneUsageDescription": "$(Swiftab) needs access to your Microphone."
      },
      "bundleIdentifier": "com.lifkha.swiftabapp",
      "runtimeVersion": "1.0.0"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#008080"
      },
      "googleServicesFile": "./google-services.json",
      "permissions": [
        "android.permission.ACCESS_COARSE_LOCATION",
        "android.permission.ACCESS_FINE_LOCATION",
        "android.permission.CAMERA",
        "android.permission.RECORD_AUDIO",
        "android.permission.ACCESS_COARSE_LOCATION",
        "android.permission.ACCESS_FINE_LOCATION",
        "android.permission.CAMERA",
        "android.permission.RECORD_AUDIO"
      ],
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyAO45-MMOSpzlpJwYPZ1r9owocITdy-_r4"
        }
      },
      "package": "com.lifkha.swiftabapp",
      "runtimeVersion": {
        "policy": "appVersion"
      }
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-notifications",
        {
          "icon": "./assets/images/notification.png",
          "color": "#ffffff",
          "defaultChannel": "default",
          "sounds": [
            "./assets/notification_sound.wav"
          ],
          "enableBackgroundRemoteNotifications": false
        }
      ],
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#008080"
        }
      ],
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Allow $(SwifTab) to use your location."
        }
      ],
      [
        "react-native-vision-camera",
        {
          "cameraPermissionText": "$(Swiftab) needs access to your Camera.",
          "enableMicrophonePermission": true,
          "microphonePermissionText": "$(Swiftab) needs access to your Microphone.",
          "enableCodeScanner": true
        }
      ],
      [
        "expo-image-picker",
        {
          "photosPermission": "Allow $(Swiftab) to access your photos.",
          "cameraPermission": "Allow $(Swiftab) to access your camera."
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "googleMapsApiKey": "AIzaSyAO45-MMOSpzlpJwYPZ1r9owocITdy-_r4",
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "2891846d-84e1-486c-bec7-b4b30bf1e78b"
      }
    },
    "owner": "lifkha",
    "updates": {
      "url": "https://u.expo.dev/2891846d-84e1-486c-bec7-b4b30bf1e78b"
    }
  }
}
